{
  "version": 2,
  "installCommand": "npm install --production",
  "buildCommand": "",
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3.1.4"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/google-sites-svg-embed.html",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|svg|ico))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}